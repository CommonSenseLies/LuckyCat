html
    head
        title API Reference -- Lucky Cat
        link(type="text/css",href="/css/ui-lightness/jquery-ui-1.8.22.custom.css",rel="stylesheet")
        script(src="/js/jquery.js")
        script(src="/js/jquery-ui-1.8.22.custom.min.js")
        script(src="/js/api_reference.js")
    body
        table(width="100%")
            tbody
                tr
                    td(width=320, height=650)
                        #module_list
                            for dos,mod in namespace_content
                                div
                                    a.mod_name(href="/"+namespace+"/"+mod) !{mod}
                                    ul
                                        for api, action in dos
                                            if action!="desc"
                                                li
                                                    a(href="/"+namespace+"/"+mod+"/"+action, target="content") !{action}
                    td
                        iframe(name="content", src="http://www.baidu.com", width="100%", height="100%")